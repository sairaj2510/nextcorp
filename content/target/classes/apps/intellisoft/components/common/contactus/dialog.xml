<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Dialog"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Panel"
                title="Tab 1">
                <items jcr:primaryType="cq:WidgetCollection">
                    <header
                        jcr:primaryType="cq:Widget"
                        fieldLabel="heading"
                        name="./head"
                        xtype="textfield"/>
                    <mobile
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Mobile"
                        name="./mobile"
                        xtype="textfield"/>
                    <fax
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Fax"
                        name="./fax"
                        xtype="textfield"/>
                    <tollfree
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Tollfree"
                        name="./tollfree"
                        xtype="textfield"/>
                    <email
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Email"
                        name="./email"
                        xtype="textfield"/>
                 </items>
            </tab1>
        </items>
    </items>
</jcr:root>
