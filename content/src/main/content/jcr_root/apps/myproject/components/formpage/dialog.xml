<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Dialog"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Panel"
                title="Tab 1">
                <items jcr:primaryType="cq:WidgetCollection">
                    <firstname
                        jcr:primaryType="cq:Widget"
                        fieldLabel="First Name"
                        name="./firstname"
                        xtype="textfield"/>
                    <lastname
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Last Name"
                        name="./lastname"
                        xtype="textfield"/>
                    <birthday
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Date Of Birth"
                        name="./birthday"
                        xtype="textfield"/>
                    <emailid
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Email Id"
                        name="./emailid"
                        xtype="textfield"/>
                    <mobilenumber
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Mobile Number"
                        name="./mobilenumber"
                        xtype="textfield"/>
                    <gender
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Gender"
                        name="./gender"
                        type="select"
                        xtype="selection">
                        <options jcr:primaryType="cq:WidgetCollection">
                            <male
                                jcr:primaryType="nt:unstructured"
                                text="Male"
                                value="male"/>
                            <female
                                jcr:primaryType="nt:unstructured"
                                text="Female"
                                value="female"/>
                        </options>
                    </gender>
                    <address
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Address"
                        name="./address"
                        xtype="textfield"/>
                    <city
                        jcr:primaryType="cq:Widget"
                        fieldLabel="City"
                        name="./city"
                        xtype="textfield"/>
                    <pincode
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Pin Code"
                        name="./pincode"
                        xtype="textfield"/>
                    <state
                        jcr:primaryType="cq:Widget"
                        fieldLabel="State"
                        name="./state"
                        xtype="textfield"/>
                    <country
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Country"
                        name="./country"
                        xtype="textfield"/>
                    <classXboard
                        jcr:primaryType="cq:Widget"
                        fieldLabel="ClassXboard"
                        name="./classXboard"
                        xtype="textfield"/>
                    <classXPercentage
                        jcr:primaryType="cq:Widget"
                        fieldLabel="ClassXPercentage"
                        name="./classXPercentage"
                        xtype="textfield"/>
                    <classXYrOfPassing
                        jcr:primaryType="cq:Widget"
                        fieldLabel="ClassXYrOfPassing"
                        name="./classXYrOfPassing"
                        xtype="textfield"/>
                    <classXIIboard
                        jcr:primaryType="cq:Widget"
                        fieldLabel="ClassXIIboard"
                        name="./classXIIboard"
                        xtype="textfield"/>
                    <classXIIPercentage
                        jcr:primaryType="cq:Widget"
                        fieldLabel="ClassXIIPercentage"
                        name="./classXIIPercentage"
                        xtype="textfield"/>
                    <classXIIYrOfPassing
                        jcr:primaryType="cq:Widget"
                        fieldLabel="ClassXIIYrOfPassing"
                        name="./classXIIYrOfPassing"
                        xtype="textfield"/>
                    <graduationboard
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Graduationboard"
                        name="./graduationboard"
                        xtype="textfield"/>
                    <graduationPercentage
                        jcr:primaryType="cq:Widget"
                        fieldLabel="GraduationPercentage"
                        name="./graduationPercentage"
                        xtype="textfield"/>
                    <graduationYrOfPassing
                        jcr:primaryType="cq:Widget"
                        fieldLabel="GraduationYrOfPassing"
                        name="./graduationYrOfPassing"
                        xtype="textfield"/>
                    <mastersboard
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Mastersboard"
                        name="./mastersboard"
                        xtype="textfield"/>
                    <mastersPercentage
                        jcr:primaryType="cq:Widget"
                        fieldLabel="MastersPercentage"
                        name="./mastersPercentage"
                        xtype="textfield"/>
                    <mastersYrOfPassing
                        jcr:primaryType="cq:Widget"
                        fieldLabel="MastersYrOfPassing"
                        name="./mastersYrOfPassing"
                        xtype="textfield"/>
                    <coursesappliedfor
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Courses Applied For"
                        name="./coursesappliedfor"
                        type="select"
                        xtype="selection">
                        <options jcr:primaryType="cq:WidgetCollection">
                            <courseBCA
                                jcr:primaryType="nt:unstructured"
                                text="BCA"
                                value="BCA"/>
                            <courseBCom
                                jcr:primaryType="nt:unstructured"
                                text="BCom"
                                value="BCom"/>
                            <courseBSc
                                jcr:primaryType="nt:unstructured"
                                text="BSc"
                                value="BSc"/>
                            <courseBA
                                jcr:primaryType="nt:unstructured"
                                text="BA"
                                value="BA"/>
                        </options>
                    </coursesappliedfor>
                </items>
            </tab1>
        </items>
    </items>
</jcr:root>
