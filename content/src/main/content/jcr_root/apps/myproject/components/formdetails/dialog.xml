<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Dialog"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Panel"
                title="Tab 1">
                <items jcr:primaryType="cq:WidgetCollection">
                    <firstname
                        jcr:primaryType="cq:Widget"
                        fieldLabel="First Name"
                        name="./FirstName"
                        xtype="textfield"/>
                    <lastname
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Last Name"
                        name="./LastName"
                        xtype="textfield"/>
                    <birthday
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Date Of Birth"
                        name="./BirthDay"
                        xtype="textfield"/>
                    <emailid
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Email Id"
                        name="./EmailId"
                        xtype="textfield"/>
                    <mobilenumber
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Mobile Number"
                        name="./MobileNumber"
                        xtype="textfield"/>
                    <gender
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Gender"
                        name="./Gender"
                        type="select"
                        xtype="selection">
                        <options jcr:primaryType="cq:WidgetCollection">
                            <male
                                jcr:primaryType="nt:unstructured"
                                text="Male"
                                value="Male"/>
                            <female
                                jcr:primaryType="nt:unstructured"
                                text="Female"
                                value="Female"/>
                        </options>
                    </gender>
                    <address
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Address"
                        name="./Address"
                        xtype="textfield"/>
                    <city
                        jcr:primaryType="cq:Widget"
                        fieldLabel="City"
                        name="./City"
                        xtype="textfield"/>
                    <pincode
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Pin Code"
                        name="./PinCode"
                        xtype="textfield"/>
                    <state
                        jcr:primaryType="cq:Widget"
                        fieldLabel="State"
                        name="./state"
                        xtype="textfield"/>
                    <country
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Country"
                        name="./Country"
                        xtype="textfield"/>
                    <hobbydrawing
                        jcr:primaryType="cq:Widget"
                        fieldLabel = "Drawing"
                        name="./HobbyDrawing"
                        xtype="textfield"/>
                    <hobbysinging
                        jcr:primaryType="cq:Widget"
                        fieldLabel = "Singing"
                        name="./HobbySinging"
                        xtype="textfield"/>
                    <hobbydancing
                        jcr:primaryType="cq:Widget"
                        fieldLabel = "Dancing"
                        name="./HobbyDancing"
                        xtype="textfield"/>
                    <hobbycooking
                        jcr:primaryType="cq:Widget"
                        fieldLabel = "Cooking"
                        name="./HobbyCooking"
                        xtype="textfield"/>
                    <classXBoard
                        jcr:primaryType="cq:Widget"
                        fieldLabel="ClassXboard"
                        name="./ClassXBoard"
                        xtype="textfield"/>
                    <classXPercentage
                        jcr:primaryType="cq:Widget"
                        fieldLabel="ClassXPercentage"
                        name="./ClassXPercentage"
                        xtype="textfield"/>
                    <classXYrOfPassing
                        jcr:primaryType="cq:Widget"
                        fieldLabel="ClassXYrOfPassing"
                        name="./ClassXYrOfPassing"
                        xtype="textfield"/>
                    <classXIIBoard
                        jcr:primaryType="cq:Widget"
                        fieldLabel="ClassXIIboard"
                        name="./ClassXIIBoard"
                        xtype="textfield"/>
                    <classXIIPercentage
                        jcr:primaryType="cq:Widget"
                        fieldLabel="ClassXIIPercentage"
                        name="./ClassXIIPercentage"
                        xtype="textfield"/>
                    <classXIIYrOfPassing
                        jcr:primaryType="cq:Widget"
                        fieldLabel="ClassXIIYrOfPassing"
                        name="./ClassXIIYrOfPassing"
                        xtype="textfield"/>
                    <graduationBoard
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Graduationboard"
                        name="./GraduationBoard"
                        xtype="textfield"/>
                    <graduationPercentage
                        jcr:primaryType="cq:Widget"
                        fieldLabel="GraduationPercentage"
                        name="./GraduationPercentage"
                        xtype="textfield"/>
                    <graduationYrOfPassing
                        jcr:primaryType="cq:Widget"
                        fieldLabel="GraduationYrOfPassing"
                        name="./GraduationYrOfPassing"
                        xtype="textfield"/>
                    <mastersBoard
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Mastersboard"
                        name="./MastersBoard"
                        xtype="textfield"/>
                    <mastersPercentage
                        jcr:primaryType="cq:Widget"
                        fieldLabel="MastersPercentage"
                        name="./MastersPercentage"
                        xtype="textfield"/>
                    <mastersYrOfPassing
                        jcr:primaryType="cq:Widget"
                        fieldLabel="MastersYrOfPassing"
                        name="./MastersYrOfPassing"
                        xtype="textfield"/>
                    <coursesappliedfor
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Courses Applied For"
                        name="./CourseAppliedFor"
                        type="select"
                        xtype="selection">
                        <options jcr:primaryType="cq:WidgetCollection">
                            <courseBCA
                                jcr:primaryType="nt:unstructured"
                                text="BCA"
                                value="BCA"/>
                            <courseBCom
                                jcr:primaryType="nt:unstructured"
                                text="B.Com"
                                value="B.Com"/>
                            <courseBSc
                                jcr:primaryType="nt:unstructured"
                                text="B.Sc"
                                value="B.Sc"/>
                            <courseBA
                                jcr:primaryType="nt:unstructured"
                                text="B.A"
                                value="B.A"/>
                        </options>
                    </coursesappliedfor>
                </items>
            </tab1>
        </items>
    </items>
</jcr:root>
